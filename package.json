    "@rollup/rollup-win32-ia32-msvc": "^4.5.0",
    "@rollup/rollup-win32-x64-msvc": "^4.5.0",
    "@esbuild/android-arm": "^0.19.6",
    "@esbuild/android-arm64": "^0.19.6",
    "@esbuild/android-x64": "^0.19.6",
    "@esbuild/darwin-arm64": "^0.19.6",
